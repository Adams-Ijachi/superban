{
  "name": "piusadams/superban",
  "description": "A Laravel package to ban users by IP address, email address, or id.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/Adams-Ijachi/superban",
  "authors": [
    {
      "role": "Developer",
      "name": "Pius Adams",
      "email": "ijachipius8@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PiusAdams\\SuperBan\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PiusAdams\\SuperBan\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PiusAdams\\SuperBan\\Providers\\SuperBanServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "pestphp/pest": "^2.28",
    "pestphp/pest-plugin-laravel": "^2.2"
  }
}
